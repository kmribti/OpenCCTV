<div class="container">
  <h1>
    Analytic instance information

    <% if isOrganization? %>
        <a href="<%=edit_group_user_analytic_instance_path(params[:group_user_id],@analytic_instance)%>" class="btn btn-warning"><span class="glyphicon glyphicon-pencil"></span> Edit</a>

  <% else %>
        <a href="<%=edit_analytic_instance_path(@analytic_instance)%>" class="btn btn-warning"><span class="glyphicon glyphicon-pencil"></span> Edit</a>
  <% end %>

  </h1>
  <p>
    <strong>Analytic ID:</strong>
    <%= @analytic_instance.id %>
  </p>
  <p>
    <strong>Name:</strong>
    <%= @analytic_instance.name %>
  </p>
  <p>
    <strong>Analytic Type:</strong>
    <%= @analytic_instance.analytic.name %>
  </p>
  <p>
    <strong>Description:</strong>
    <%= @analytic_instance.description %>
  </p>

  <p>
    <strong>Analytic server id:</strong>
    <%= @analytic_instance.analytic_server_id %>
  </p>
</div>

